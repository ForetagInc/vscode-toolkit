{
	"name": "development-toolkit",
	"displayName": "Foretag Development Toolkit",
	"description": "Foretag themes, tools & extensions for VS Code",
	"version": "0.0.8",
	"keywords": [
		"foretag"
	],
	"publisher": "foretag",
	"icon": "../../assets/images/foretag_logo_yellow.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ForetagInc/vscode-toolkit.git"
	},
	"engines": {
		"vscode": "^1.66.1"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Extension Packs",
		"Visualization",
		"Other"
	],
	"extensionPack": [
		"foretag.icons",
		"foretag.themes",
		"foretag.rust-toolkit",
		"rust-lang.rust-analyzer",
		"serayuzgur.crates",
		"vadimcn.vscode-lldb",
		"masterco.master-css-language-service",
		"tamasfe.even-better-toml",
		"redhat.vscode-xml",
		"redhat.vscode-yaml",
		"dtsvet.vscode-wasm",
		"editorconfig.editorconfig",
		"please-build.plz-vscode",
		"ms-kubernetes-tools.vscode-kubernetes-tools",
		"ms-azuretools.vscode-docker",
		"GraphQL.vscode-graphql",
		"tauri-apps.tauri-vscode",
		"Slint.slint"
	],
	"contributes": {
		"languages": [
			{
				"id": "dotenv",
				"extensions": [
					".flaskenv",
					".env",
					".env-sample",
					".env.example",
					".env.template",
					".env.local",
					".env.dev",
					".env.dev.local",
					".env.build",
					".env.build.local",
					".env.development",
					".env.development.local",
					".env.test",
					".env.test.local",
					".env.testing",
					".env.qa",
					".env.qa.local",
					".env.uat",
					".env.uat.local",
					".env.stag",
					".env.stag.local",
					".env.stage",
					".env.stage.local",
					".env.staging",
					".env.staging.local",
					".env.live",
					".env.live.local",
					".env.production",
					".env.production.local",
					".env.prod",
					".env.prod.local"
				],
				"filenamePatterns": [
					"?*env"
				],
				"filenames": [
					"env"
				],
				"configuration": "./languages/env/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dotenv",
				"scopeName": "source.env",
				"path": "./languages/env/syntaxes/env.tmLanguage"
			}
		],
		"iconThemes": [
			{
				"id": "foretag-default-icons",
				"label": "Foretag: Default",
				"path": "./icons/default-icons.json"
			}
		]
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"typescript": "^4.6.3"
	}
}